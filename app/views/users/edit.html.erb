<h1>プロフィール編集</h1>

<% if @user.errors.any? %>
  <%= @user.errors.count %> error prohibited this obj from being saves:
  <ul>
    <% @user.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
<% end %>

<%= form_with model: @user do |f| %>

  <p>氏名<br>
  <%= f.text_field :name %></p>

  <p>所属<br>
  <%= f.text_field :team %></p>

  <p>電話番号<br>
  <%= f.text_field :telephone_number %></p>

  <p>メールアドレス<br>
  <%= f.text_field :email %></p>

  <%= f.submit "変更を保存" %>
  
<% end %>